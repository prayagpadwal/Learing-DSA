    "def arrayPairSum(nums: list[int]) -> int:\n",
    "        \n",
    "    nums = sorted(nums)\n",
    "    sum = 0 \n",
    "\n",
    "    for i in range(0, len(nums), 2):\n",
    "        sum = sum + nums[i]\n",
    "    return sum\n",
    "        \n",
    "\n",
    "result = arrayPairSum([1, 4, 3, 2]) # Expect output is 4\n",
    "print(result)"
   ]
